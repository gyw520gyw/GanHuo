<?xml version="1.0" encoding="utf-8"?>
<resources>



    <!-- TopBar -->
    <declare-styleable name="TopBar">
        <attr name="leftSrc" format="reference"></attr>
        <attr name="centerText" format="string"></attr>
        <attr name="rightSrc" format="reference"></attr>
        <attr name="isShowLeft" format="boolean"></attr>
        <attr name="isShowRight" format="boolean"></attr>
        <attr name="barBg" format="reference|color"></attr>
    </declare-styleable>


    <!--Roundedimageview-->
    <declare-styleable name="Roundedimageview">
        <attr name="roundedBorderThickness" format="dimension" />
        <attr name="roundedBorderColor" format="color" />
    </declare-styleable>

</resources>